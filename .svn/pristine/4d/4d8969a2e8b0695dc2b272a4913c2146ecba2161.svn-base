-- Table structure for hostial
-- ----------------------------
DROP TABLE IF EXISTS `hostial`;
CREATE TABLE `hostial` (
  `hos_id` int(10) NOT NULL AUTO_INCREMENT,
  `hos_name` varchar(50) DEFAULT NULL COMMENT '医院名称',
  `hos_description` varchar(225) DEFAULT NULL COMMENT '医院描述',
  `hos_location` varchar(50) DEFAULT NULL COMMENT '医院地点',
  `hos_longitude` varchar(10) DEFAULT NULL COMMENT '医院经度',
  `hos_latitude` varchar(10) DEFAULT NULL COMMENT '医院纬度',
  `hos_img1` varchar(10) DEFAULT NULL COMMENT '医院图片1',
  `hos_img2` varchar(10) DEFAULT NULL COMMENT '医院图片2',
  `hos_img3` varchar(10) DEFAULT NULL,
  `hos_img4` varchar(10) DEFAULT NULL,
  `hos_brief` varchar(255) DEFAULT NULL,
  `hos_address` varchar(255) DEFAULT NULL,
  `hos_path` varchar(255) DEFAULT NULL,
  PRIMARY KEY (`hos_id`)
) ENGINE=InnoDB AUTO_INCREMENT=9 DEFAULT CHARSET=utf8;

