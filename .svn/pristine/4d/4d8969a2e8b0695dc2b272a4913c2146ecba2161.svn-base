-- ----------------------------
-- Table structure for hos_doctor
-- ----------------------------
DROP TABLE IF EXISTS `hos_doctor`;
CREATE TABLE `hos_doctor` (
  `doc_member_id` int(11) NOT NULL AUTO_INCREMENT,
  `hos_id` int(11) DEFAULT NULL,
  `doc_image` varchar(255) DEFAULT NULL COMMENT '医师介绍',
  `doc_depart` varchar(255) DEFAULT NULL,
  `doc_position` varchar(255) DEFAULT NULL,
  `doc_id` int(10) DEFAULT NULL,
  `doc_direction` varchar(255) DEFAULT NULL COMMENT '擅长疾病',
  `doc_background` varchar(255) DEFAULT NULL COMMENT '教育背景',
  `doc_evaluate1` int(11) DEFAULT NULL,
  `doc_evaluate2` int(11) DEFAULT NULL,
  `doc_evaluate3` int(11) DEFAULT NULL,
  `doc_evaluate4` int(11) DEFAULT NULL,
  `doc_hospital` varchar(255) DEFAULT NULL,
  `doc_adept` varchar(255) DEFAULT NULL,
  `doc_label1` varchar(255) DEFAULT NULL COMMENT '标签1',
  `doc_label2` varchar(255) DEFAULT NULL COMMENT '标签2',
  PRIMARY KEY (`doc_member_id`),
  KEY `hos_id` (`hos_id`),
  KEY `doc_id` (`doc_id`),
  CONSTRAINT `doc_id` FOREIGN KEY (`doc_id`) REFERENCES `user` (`user_id`),
  CONSTRAINT `hos_id` FOREIGN KEY (`hos_id`) REFERENCES `hostial` (`hos_id`)
) ENGINE=InnoDB AUTO_INCREMENT=5 DEFAULT CHARSET=utf8;

